                                    </a>
                                </li>
                            }
                            else
                            {
                                <li>
                                    <a asp-controller="Courses" asp-action="Index" asp-route-categoryId="@category.Id">
                                        <div class="course__sidebar-check mb-10 d-flex align-items-center">
                                            <input class="m-check-input" type="checkbox" id="m-eng" disabled>
                                            <label class="m-check-label" @*for="m-eng"*@>@category.CategoryName</label>
                                        </div>
                                    </a>
                                </li>
                            }
                            
                        }
                    }
                </ul>
            </div>
        </div>
        <div class="course__sidebar-widget white-bg">
            <div class="course__sidebar-course">
                <h3 class="course__sidebar-title">Related courses</h3>
                <ul>
                    @{
                        var count = 0;
                        foreach (var rCourse in Model.RelatedCourses)
                        {
                            <li>
                                <div class="course__sm d-flex align-items-center mb-30">
                                    <div class="course__sm-thumb mr-20">
                                        <a asp-controller="Course" asp-action="Index" asp-route-id="@rCourse.Id">
                                            <img src="@rCourse.ImgUrl" alt="">
                                        </a>
                                    </div>
                                    <div class="course__sm-content">
                                        <div class="course__sm-rating">
                                            <ul>
                                                <li>
                                                    <a href="#">
                                                        <i class="fa-solid fa-star"></i>
                                                    </a>
                                                </li>
                                                <li>
                                                    <a href="#">
                                                        <i class="fa-solid fa-star"></i>
                                                    </a>
                                                </li>
                                                <li>
                                                    <a href="#">
                                                        <i class="fa-solid fa-star"></i>
                                                    </a>
                                                </li>
                                                <li>
                                                    <a href="#">
                                                        <i class="fa-solid fa-star"></i>
                                                    </a>
                                                </li>
                                                <li>
                                                    <a href="#">
                                                        <i class="fa-solid fa-star"></i>
                                                    </a>
                                                </li>
                                            </ul>
                                        </div>
                                        <h5>
                                            <a asp-controller="Course" asp-action="Index" asp-route-id="@rCourse.Id">@rCourse.Category.CategoryName</a>
                                        </h5>
                                        <div class="course__sm-price">
                                            <span>$@rCourse.Price</span>
                                        </div>
                                    </div>
                                </div>
                            </li>
                            count++;
                            if (count == 3)
                                break;
                        }
                    }

                </ul>
            </div>
        </div>
    </div>
</div>
</div>
</div>
</section>
<!-- course area end -->

</main>